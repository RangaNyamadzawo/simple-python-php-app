{
  "AWSEBDockerrunVersion": 3,
  "services": {
    "backend": {
      "build": "./backend",
      "ports": ["5000:5000"],
      "environment": {
        "FLASK_ENV": "production"
      }
    },
    "frontend": {
      "build": "./frontend", 
      "ports": ["80:80"],
      "depends_on": ["backend"],
      "environment": {
        "BACKEND_URL": "http://backend:5000"
      }
    }
  }
}